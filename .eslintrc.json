{
    "globals": {
        "require": true,
        "console": true
    },
    "rules": {
        "comma-dangle": 1,
        "no-cond-assign": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": [1, "both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 2,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-typeof": 2,
        "block-scoped-var": 1,
        "complexity": 1,
        "curly": 1,
        "default-case": 1,
        "eqeqeq": [1, "allow-null"],
        "guard-for-in": 1,
        "no-alert": 1,
        "no-div-regex": 1,
        "no-else-return": 1,
        "no-eq-null": 1,
        "no-eval": 2,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-implied-eval": 1,
        "no-invalid-this": 1,
        "no-lone-blocks": 1,
        "no-new-func": 1,
        "no-new": 1,
        "no-octal": 1,
        "no-redeclare": [1, {
            "builtinGlobals": true
        }],
        "no-self-compare": 1,
        "wrap-iife": 1,
        "yoda": 1,
        "strict": [1, "global"],
        "no-delete-var": 1,
        "no-undef": 1,
        "no-unused-vars": 1,
        "array-bracket-spacing": 1,
        "camelcase": 1,
        "comma-spacing": 1,
        "eol-last": 1,
        "max-nested-callbacks": [1, 3],
        "new-cap": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-underscore-dangle": 1,
        "quotes": [1, "double"],
        "semi": 2
    }
}
